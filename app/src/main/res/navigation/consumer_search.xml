<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/consumer_search_navigation"
    app:startDestination="@+id/consumer_search_dest">

    <fragment
        android:id="@+id/consumer_search_dest"
        android:name="com.example.allfavour.ui.consumer.search.SearchFragment"
        android:label="Search"
        tools:layout="@layout/consumer_search_fragment">
        <action
            android:id="@+id/action_consumer_search_dest_to_favourFragment"
            app:destination="@id/favourFragment">

            <argument
                android:name="fragmentId"
                app:argType="string" />

        </action>
    </fragment>

    <fragment
        android:id="@+id/favourFragment"
        android:name="com.example.allfavour.ui.consumer.favour.FavourFragment"
        android:label="consumer_favour_fragment"
        tools:layout="@layout/consumer_favour_fragment" />

</navigation>